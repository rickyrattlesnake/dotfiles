{
  "editor.detectIndentation": false,
  "editor.foldingStrategy": "indentation",
  "editor.fontFamily": "FiraCode-Retina, Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.showFoldingControls": "always",
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.formatOnSaveTimeout": 1500,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "typescript": "tsx",
    "xml": {
      "attr_quotes": "single"
    }
  },
  "files.associations": {
    "*.cpt": "gauge",
    "*.spec": "gauge",
    "*.yaml.*": "yaml",
    "Makefile.*": "makefile"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/build/**": true,
    "**/coverage/**": true,
    "**/node_modules/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "gitlens.codeLens.enabled": false,
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  // "jest.autoEnable": false,
  "launch": {
    "compounds": [],
    "configurations": [
      {
        "console": "integratedTerminal",
        "disableOptimisticBPs": true,
        "internalConsoleOptions": "neverOpen",
        "name": "Jest Current File",
        "port": 9301,
        "request": "launch",
        "runtimeArgs": [
          "--inspect=9301",
          "${workspaceRoot}/node_modules/.bin/jest",
          "--runInBand",
          "--no-cache",
          "--no-watch",
          "${relativeFile}"
        ],
        "runtimeExecutable": "/Users/rickyratnayake/.nvm/versions/node/v12.14.0/bin/node",
        "type": "node"
      },
      {
        "internalConsoleOptions": "neverOpen",
        "name": "Node Server Attach",
        "port": 9229,
        "request": "attach",
        "type": "node"
      }
    ]
  },
  "[ruby]": {
    "editor.formatOnSave": false
  },
  "ruby.format": "rubocop",
  "ruby.lint": {
    "rubocop": {
      "useBundler": true
    }
  },
  "ruby.lintDebounceTime": 500,
  "ruby.intellisense": "rubyLocate",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "scss.lint.argumentsInColorFunction": "ignore",
  "scss.lint.hexColorLength": "ignore",
  "solargraph.formatting": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.shell.linux": "/usr/local/bin/zsh",
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Material Theme Ocean",
  "files.trimTrailingWhitespace": true,
  "git.enableSmartCommit": true,
  "[yaml]": {
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[NGINX]": {},
  "terminal.integrated.rendererType": "dom",
  "workbench.iconTheme": "material-icon-theme",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/*.code-search": false
  },
  "files.exclude": {
    "**/coverage": true
  },
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative",
  "[dockerfile]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[makefile]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 2,
    "editor.renderWhitespace": "boundary"
  },
  "editor.renderWhitespace": "none",
  "java.configuration.checkProjectSettingsExclusions": false,
  "typescriptHero.imports.disableImportsSorting": true,
  "typescriptHero.imports.organizeOnSave": true
}
